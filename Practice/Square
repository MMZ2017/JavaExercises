package Practice;

public class Square implements IShape, IPolygon {
	double sideLength;

	@Override
	public void getPerimeter() {
		System.out.println(4*sideLength);
	}

	@Override
	public void getArea() {
		System.out.println(Math.pow(sideLength, 2));
	}

	@Override
	public int getNumOfSide() {
		return 4;
	}

}
